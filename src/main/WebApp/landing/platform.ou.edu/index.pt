	<script type="text/javascript">
		$(document).ready(function() {
			var conversion = '//insight.adsrvr.org/track/conv/?adv=wlk6kxt&ct=0:tiw55lll&fmt=3'

			$('a.track').click(function(event) {
				event.stopPropagation();
				event.preventDefault();
				var href = this.href;

				function go() {
					location.href = href;
				}

				var img = new Image();
				img.onerror = go;
				img.onload = go;
				img.src = conversion;
			});
			$(document).foundation();

			var doc = document.documentElement;
			doc.setAttribute('data-useragent', navigator.userAgent);
		})
	</script>

